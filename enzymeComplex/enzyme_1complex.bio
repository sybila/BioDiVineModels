VARS:S,E,ES,P

PARAMS: k1,0.001,1

EQ:S = (-k1*S*E) + 0.1*ES
EQ:E = 0.1*ES + (-k1*S*E) + 0.01*ES - 0.001*E*P
EQ:ES = (-0.1)*ES + k1*E*S + (-0.01)*ES + 0.001*E*P
EQ:P = 0.01*ES - 0.001*E*P

THRES:S:0,0.01,25,50,75,100
THRES:E:0,0.01,10,20,35,50
THRES:ES:0,0.01,5,10,20,40,60
THRES:P:0,0.01,10,20,30,40,45,50,60,80

INIT: S:75,100; E:35,50; ES:0,0.01; P:0,0.01

system async;
